# Docker Compose Environment Variables
# Copy this file to .env and update with your values
# This file is used by docker-compose.yml

# PostgreSQL - Product Order Database
POSTGRES_PRODUCT_USER=postgres
POSTGRES_PRODUCT_PASSWORD=postgres
POSTGRES_PRODUCT_DB=product_order_db
POSTGRES_PRODUCT_PORT=5432

# PostgreSQL - Customer Database
POSTGRES_CUSTOMER_USER=postgres
POSTGRES_CUSTOMER_PASSWORD=postgres
POSTGRES_CUSTOMER_DB=customer_db
POSTGRES_CUSTOMER_PORT=5433

# RabbitMQ
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

